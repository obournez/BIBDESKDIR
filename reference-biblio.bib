@phdthesis{Cam01-f,
	_bib2doi_finished = {true},
	abstract = {The theory of analog computation, where the internal states of a computer are continuous rather than discrete, has enjoyed a recent resurgence of interest. Here, we explore recursion theory on the reals, the analog counterpart of recursive function theory. There, the discrete operations, such as primitive recursion or bounded sums and products, are replaced by operations on continuous functions, such as integration or linear integration. We define recursive classes of real valued functions similarly to the classical approach in recursion theory and we study those classes to understand the computational power of operators that map real valued functions into real valued functions. We prove both upper and lower bounds for several continuous-time analog systems, which lie inside the primitive recursive functions and can be, therefore, characterized in terms of standard computational complexity classes. We also propose a theory of analog circuits. We prove upper and lower computational bounds for these circuits, depending on the kind of processing units they contain and on the topology of their underlying graphs. Namely, we show that circuits with units that solve differential equations, simple units that add and multiply functions, and units that sense inequalities in a differentiable way, can compute sets of functions that range from the functions computable in linear space to the primitive recursive functions.},
	author = {Campagnolo, Manuel L.},
	date-added = {2024-04-24 21:47:18 +0200},
	date-modified = {2024-04-24 21:47:18 +0200},
	keywords = {Analog computation, recursive functions, differential equations, dynamical systems, Grzegorczyk hierarchy, Ritchie hierarchy},
	local-url = {file://localhost/Users/bournez/BASE-DE-DONNEES/base-devon-pro.dtBase/Files/Cam01.pdf},
	school = {IST, Universidade T{\'e}cnica de Lisboa},
	title = {Computational complexity of real valued recursive functions and analog circuits},
	type = {Th{\`e}se de doctorat},
	year = {2001}}

