	keywords = {Automates cellulaires, g{\'e}om{\'e}trie, mod{\`e}le du calcul continu, machine {\`a} signaux, signaux},
	local-url = {file://localhost/Users/bournez/BASE-DE-DONNEES/base-devon-pro.dtBase/Files/Dur03.pdf},
	month = {dec},
